    <%- include('partial/head-bootstrap') %>
    <title>Document</title>
</head>
<body>
    <h1>ADD Project</h1>
    <br>
    <form action="/company/<%= comId %>/project/add" method="post">
        <div>
            <label>Name Project</label>
            <input type="text" name="nameProject">
        </div>
        <div>
            <label>Minimum Load</label>
            <input type="text" name="minimumLoad">
        </div>
        <div>
            <label>Detail</label>
            <input type="text" name="detail">
        </div>
        <div>
            <label>Image URL</label>
            <input type="text" name="imageURL">
        </div>
        <button type="submit">ADD Project</button>
    </form>
</body>
</html>